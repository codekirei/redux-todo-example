{
  "name": "redux-todo-example",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "scripts/dev-server.js",
    "test": "mocha",
    "test:verbose": "npm run -s test -- -R spec",
    "cover": "istanbul cover --config configs/istanbul.yml _mocha",
    "cover:report": "cat coverage/lcov.info | lcov-summary",
    "clean": "npm-run-all clean:*",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "build": "npm-run-all clean:dist build:compile",
    "build:compile": "scripts/build.js",
    "serve:coverage": "scripts/coverage-server.js",
    "serve:build": "browser-sync start --server dist --no-open",
    "lint": "npm-run-all lint:styles lint:scripts",
    "lint:styles": "stylelint --config configs/stylelint.yml 'src/styles/**/*.css'",
    "lint:scripts": "eslint '{src,scripts,test}/**/*.js?(x)'",
    "dev": "chokidar 'src/**/*.{js,jsx,html}' -c 'clear && npm-run-all lint test'",
    "dev:test": "chokidar 'src/**/*.{js,jsx,html}' 'test/**/*.{js,jsx}' -c 'clear && npm run cover'"
  },
  "author": "Jacob Blakely (codekirei)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.7.2",
    "browser-sync": "2.12.1",
    "chokidar-cli": "1.2.0",
    "clear-cli": "1.0.1",
    "css-loader": "0.23.1",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "7.0.0",
    "eslint-plugin-jsx-a11y": "0.6.2",
    "eslint-plugin-react": "4.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-minifier": "1.5.0",
    "ignore-styles": "1.2.0",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "8.3.1",
    "kirei-css": "0.1.1",
    "lcov-summary": "1.0.1",
    "mocha": "2.4.5",
    "mocha-tldr-reporter": "1.0.0",
    "npm-run-all": "1.7.0",
    "postcss-import": "8.1.0",
    "postcss-loader": "0.8.2",
    "react-addons-test-utils": "15.0.1",
    "redux-devtools": "3.2.0",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.9",
    "redux-immutable-state-invariant": "1.2.2",
    "rimraf": "2.5.2",
    "sinon": "1.17.3",
    "style-loader": "0.13.1",
    "stylelint": "5.4.0",
    "stylelint-config-standard": "5.0.0",
    "webpack": "1.12.15",
    "webpack-dev-middleware": "github:webpack/webpack-dev-middleware",
    "webpack-hot-middleware": "2.10.0"
  },
  "dependencies": {
    "node-uuid": "1.4.7",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-redux": "4.4.4",
    "redux": "3.4.0"
  }
}
